{
  "expo": {
    "name": "UstazUlytauExpo",
    "slug": "UstazUlytauExpo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Приложение запрашивает доступ к камере устройства для того, чтобы сделать фотографию профиля.",
        "NSPhotoLibraryUsageDescription": "Приложение запрашивает доступ к фотоальбому для выбора фотографии профиля из галереи."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]

    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Приложение запрашивает доступ к фотоальбому для выбора фотографии профиля из галереи.",
          "cameraPermission": "Приложение запрашивает доступ к камере устройства для того, чтобы сделать фотографию профиля."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f8283183-a3c9-4f98-a5c5-cd95f6416354"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/f8283183-a3c9-4f98-a5c5-cd95f6416354"
    }
  }
}
